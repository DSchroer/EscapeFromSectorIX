if(move)
{

    if(collision_line(x,y,nx_x,nx_y,wall,true,true))
    {
        if(collision_point(nx_x,y,wall,true,true))
        {
            nx_x = x
            xhit = true
        }
        
        if(collision_point(x,nx_y,wall,true,true))
        {
            nx_y = y
            yhit = true
        }
        
