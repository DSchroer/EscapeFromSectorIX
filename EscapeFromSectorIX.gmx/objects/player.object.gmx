{
    if(nx_x - x &gt; mv_speed)
    {
        x += mv_speed
    }else{
        x = nx_x
    }
}else if(nx_x &lt; x)
{
    if(x - nx_x &gt; mv_speed)
    {
        x -= mv_speed
    }else{
        x = nx_x
    }
}

if(nx_y &gt; y)
{
    if(nx_y - y &gt; mv_speed)
    {
        y += mv_speed
    }else{
        y = nx_y
    }
}else if(nx_y &lt; y)
{
    if(y - nx_y &gt; mv_speed)
    {
        y -= mv_speed
    }else{
        y = nx_y
    }
}
